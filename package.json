{
  "name": "debug-logger",
  "version": "1.0.0",
  "description": "A web application for visualizing debug logs from other applications",
  "repository": "git@github.com:Spidy88/debug-logger.git",
  "author": "Nick Ferraro <the.nick.ferraro@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn run build:client && yarn run build:server",
    "start": "node dist/server",
    "build:client": "npx webpack --config webpack.prod.js",
    "build:server": "npx babel server/index.js -d dist/server",
    "dev:client": "npx webpack --config webpack.dev.js --watch",
    "dev:server": "npx nodemon --watch server --watch config --exec \"yarn run build:server && yarn start\"",
    "lint": "npx eslint --max-warnings=0 \"client/**/*.js\" \"server/**/*.js\" \"tests/**/*.js\"",
    "test": "npx jest --env=jsdom tests/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/config/setupEnzyme.js"
    ],
    "testMatch": [
      "<rootDir>/tests/unit/**/*.{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    }
  },
  "dependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.4",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "clean-webpack-plugin": "1.0.0",
    "config": "3.0.1",
    "css-loader": "2.1.0",
    "css-reset-and-normalize": "1.0.1",
    "dotenv": "6.2.0",
    "evergreen-ui": "4.8.0",
    "express": "4.16.4",
    "html-webpack-plugin": "3.2.0",
    "lightship": "2.0.0",
    "lodash": "4.17.11",
    "logrocket": "0.6.19",
    "logrocket-react": "3.0.0",
    "moment": "2.23.0",
    "node-sass": "4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "react-svg-loader": "2.1.0",
    "redis": "2.8.0",
    "redux": "4.0.1",
    "redux-actions": "2.6.4",
    "redux-logger": "3.0.6",
    "sass-loader": "7.1.0",
    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.1.1",
    "ui-box": "1.4.0",
    "webpack": "4.28.3",
    "webpack-cli": "3.1.2",
    "webpack-merge": "4.1.5",
    "winston": "3.1.0"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.11.1",
    "eslint-plugin-react": "7.12.0",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "jest-svg-transformer": "1.0.0",
    "nodemon": "1.18.9",
    "react-test-renderer": "16.7.0",
    "timezone-mock": "1.0.2"
  }
}
